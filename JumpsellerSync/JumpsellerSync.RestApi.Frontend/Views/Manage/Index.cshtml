
@{
   ViewData["Title"] = "Administrar";
}

<h1>Administrar Productos Vtex</h1>

<div class="row" id="manage-actions">
   <button type="button" class="btn btn-primary" id="load-jumpseller-products">
      <i class="fa fa-download"></i>
      Cargar Productos
   </button>

   <button type="button" class="btn btn-outline-danger" id="purge-jumpseller-products">
      <i class="fa fa-trash"></i>
      Purgar Productos
   </button>

   <div class="spinner-border text-primary" role="status" id="manage-spinner">
      <span class="sr-only">Cargando...</span>
   </div>

</div>

<div class="row" id="manage-content">

</div>

@section Styles {
   <environment include="Development">
      <link rel="stylesheet" href="~/css/manage.css" />
   </environment>
   <environment exclude="Development">
      <link rel="stylesheet" href="~/bundles/css/manage.min.css" />
   </environment>
}

@section Scripts {
   <environment include="Development">
      <script src="~/js/manage.js"></script>
   </environment>
   <environment exclude="Development">
      <script src="~/bundles/js/manage.min.js"></script>
   </environment>
}
