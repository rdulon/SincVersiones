@model JumpsellerSync.BusinessLogic.Core.Dtos.Main.ProviderDto

@{
   ViewData["Title"] = "Editar Proveedor";
   ViewBag.Action = "Edit";
   ViewBag.ActionText = "Grabar Proveedor";
}

@{ await Html.RenderPartialAsync("CreateEdit", Model); }

@section Styles {
   <environment include="Development">
      <link rel="stylesheet" href="~/css/provider.css" />
